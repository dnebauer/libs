local rc=1

_arguments -S \
  - '(help_options)' \
    '-h[Show a compact help message]' \
    '--help[Show a long help message]' \
    '--usage[Show a short help message]' \
    '--man[Show the manual]' \
  - 'script_options' \
    '(-w --width)'{-w,--width}'[Resize width (px)]:resize width:' \
    '(-t --height)'{-t,--height}'[Resize height (px)]:resize height:'\
    '(-f --fill_color)'{-f,--fill_color}'[ImageMagick color (default\: none)]:colors:_fill' \
    '(-i --ignore_aspect_ratio)'{-i,--ignore_aspect_ratio}'[Stretch image to fit resized dimensions]' \
    '(-s --border_horizontal)'{-s,--border_horizontal}'[Left and right border size (px) (default\: 0)]:horizontal border:' \
    '(-v --border_vertical)'{-v,--border_vertical}'[Top and bottom border size (px) (default\: 0)]:vertical border:' \
    '*:File(s) to resize:_files' \
  && rc=0

return rc
